package com.demo.javaprj;
//Encapsulation 
public class Person {
	//int height; //instance variable or class variable
	//int weight;
	private String name; // property
	int age;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {  String name -local variable
		this.name = name;
	}
	
	
	
	
}
